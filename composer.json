{
	"name": "icanboogie/i18n",
	"type": "library",
	"description": "I18n framework",
	"keywords": [
		"i18n"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/I18n/issues",
		"source": "https://github.com/ICanBoogie/I18n"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.6",
		"icanboogie/cldr": "^1.4|^2.0",
		"icanboogie/prototype": "^3.0|^4.0|^5.0",
		"olvlvl/filecache": "dev-master"
	},
	"require-dev": {
		"icanboogie/icanboogie": "^4.0",
		"icanboogie/bind-cldr": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\": "lib"
		},
		"classmap": [
			"lib/I18n.php"
		],
		"files": [
			"helpers.php"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	},
	"extra": {
		"icanboogie": {
			"autoconfig-filters": [
				"ICanBoogie\\I18n\\Hooks::filter_autoconfig"
			],
			"config-path": "config",
			"locale-path": "locale"
		}
	}
}
