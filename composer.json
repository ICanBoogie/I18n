{
	"name": "icanboogie/i18n",
	"type": "library",
	"description": "I18n framework",
	"keywords": ["i18n"],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/I18n/issues",
		"source": "https://github.com/ICanBoogie/I18n"
	},
	"require": {
		"php": ">=5.4.0",
		"icanboogie/cldr": ">=1.3.4",
		"icanboogie/prototype": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\I18n\\": "lib"
		},
		"classmap": [ "lib/I18n.php" ],
		"files": [ "bootstrap.php", "helpers.php" ]
	},
	"extra": {
		"icanboogie": {
			"autoconfig-filters": [ "ICanBoogie\\I18n\\Hooks::filter_autoconfig" ]
		}
	}
}
